{
	"name": "@porto/js-library-corp-hubv-porto-ocean",
	"version": "0.0.3",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"type": "module",
	"files": [
		"dist",
		"src",
		"README.md"
	],
	"scripts": {
		"build": "tsup src/index.ts src/cli/cli.ts --format esm --dts --out-dir dist --minify --clean",
		"build:cli": "tsup src/cli/cli.ts --format esm --out-dir dist --minify --clean && npm run copy-files",
		"test": "jest --passWithNoTests",
		"test:cli": "node dist/cli/cli.js",
		"copy-files": "node scripts/copy-files.js",
		"clean": "npm run clean:coverage && npm run clean:dist",
		"clean:coverage": "rimraf coverage",
		"clean:dist": "rimraf dist",
		"prepack": "clean-package",
		"postpack": "clean-package restore"
	},
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/fs-extra": "^11.0.4",
		"@types/jest": "^29.5.12",
		"@types/react": "^19.1.0",
		"clean-package": "^2.2.0",
		"jest": "^29.7.0",
		"react": "^19.1.0",
		"rimraf": "^5.0.5",
		"ts-jest": "^29.1.2",
		"ts-node": "^10.9.2",
		"tsup": "^8.0.2",
		"typescript": "^5.4.5"
	},
	"dependencies": {
		"@inquirer/prompts": "^7.4.1",
		"@porto-ocean/utils": "^1.0.9",
		"commander": "^13.1.0",
		"fs-extra": "^11.3.0"
	}
}