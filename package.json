{
  "name": "@porto/js-library-corp-hubv-porto-ocean",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "bin": {
    "porto-ocean": "./dist/cli.cjs"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md"
  ],
  "author": {
    "name": "Henrique Le√£o",
    "email": "henrique.leao@portoseguro.com.br"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:cli && npm run build:components && npm run copy-files",
    "build:lib": "tsup --config tsup.config.ts",
    "build:cli": "tsup src/cli/cli.ts --format cjs --out-dir dist --minify --clean --external react,react-dom,sanitize-html,aes-js,crypto-js,next,next/image,next/link,next/navigation && node fix-cli.cjs",
    "build:components": "node scripts/build-components.js",
    "test": "jest --passWithNoTests",
    "test:cli": "node dist/cli.cjs",
    "copy-files": "node scripts/copy-files.js",
    "update-changelog": "node scripts/update-changelog.js",
    "clean": "npm run clean:coverage && npm run clean:dist",
    "clean:coverage": "rimraf coverage",
    "clean:dist": "rimraf dist",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "^7.4.1",
    "commander": "^13.1.0",
    "fs-extra": "^11.3.0",
    "lucide-react": "^0.511.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/aes-js": "^3.1.4",
    "@types/crypto-js": "^4.2.2",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.5",
    "@types/sanitize-html": "^2.16.0",
    "aes-js": "^3.1.2",
    "clean-package": "^2.2.0",
    "crypto-js": "^4.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "next": "^15.3.2",
    "react": "^19.1.0",
    "rimraf": "^5.0.5",
    "sanitize-html": "^2.17.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  }
}
